int i,j,c=1;
for(i=1;i<=4;i++)
{
    for(j=1;j<=i;j++)
    { 
        System.out.print(c+"\t");
        c++;

    }
    System.out.println();
}


or


int i,j=1;
for(i=1;i<=4;i++)
{
    for(;j<=i*(i+1)/2;j++)
    { 
        System.out.print(j+"\t");
        c++;

    }
    System.out.println();
}

