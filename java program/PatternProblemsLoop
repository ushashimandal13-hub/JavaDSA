int i,j;
for(i=4;i>=1;i--)
{
    for(j=1;j<=i;j++)
    {
       System.out.print(j);
    }
    if(i==4)
    {
        for(j=i-1;j>=1;j--)
        {
        System.out.print(j);
        }
        System.out.println();
        continue;
    }
    for(j=1;j<=2*(4-i)-1;j++)
    {
        System.out.print(" ")
    }
    for(j=i;j>=1;j--)
    {
      System.out.print(j);
    }
    System.out.println();
}








int i,j,c;
for(i=4;i>=1;i--)
{
    for(j=1;j<=i;j++)
    {
       System.out.print(j);
    }
    c=i
    if(i==4)
    {
        c=i-1;
    }
    
    for(j=1;j<=2*(4-i)-1;j++)
    {
        System.out.print(" ")
    }
    for(j=c;j>=1;j--)
    {
      System.out.print(j);
    }
    System.out.println();
}


