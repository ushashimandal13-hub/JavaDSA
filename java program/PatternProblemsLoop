int i,j;
for(i=4;i>=1;i--)
{
    for(j=1;j<=i;j++)
    {
       System.out.print(j);
    }
    for(j=1;j<=2*(4-i);j++)
    {
        System.out.print(" ")
    }
    for(j=i;j>=1;j--)
    {
      System.out.print(j);
    }
    System.out.println();
}
for(i=1;i<=4;i++)
{
    for(j=1;j<=i;j++)
    {
       System.out.print(j);
    }
    for(j=1;j<=2*(4-i);j++)
    {
        System.out.print(" ")
    }
    for(j=i;j>=1;j--)
    {
      System.out.print(j);
    }
    System.out.println();
}


