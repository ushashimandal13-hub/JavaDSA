 class Test
{
	int x,y;
	  Test(int a,int b)
	  {
          x=a;
          y=b;
	  }
		public static void main(String arg[])
		{
			
			Test t=new Test(1,2);
			Test t1=new Test(3,4);
			Test t2=new Test(5,6);
			System.out.println(t.x+" "+t.y);
			System.out.println(t1.x+" "+t1.y);
			System.out.println(t2.x+" "+t2.y);
}


