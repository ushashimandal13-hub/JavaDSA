constructor overoading 
______________________
 class Test
{
	int x,y;
	  Test()
	  {
         

	  }
	  Test(int a,int b)
	  {
	  	x=a;
	  	y=b;
	  }
		public static void main(String arg[])
		{
			
			Test t=new Test();
			
	       Test t1=new Test(1,2);
	       System.out.println(t.x+" "+t.y);
	       System.out.println(t1.x+" "+t1.y);
			
		}
}


 class Rectangle
 {
 	private int L,B;
 	Rectangle()
 	{

 	}
 	Rectangle(int x,int y)
 	{
        L=x;
        B=y;
 	}
    void show()
    {
        System.out.println("length="+L);
        System.out.println("breadth="+B);
    }
    int area()
    {
    	return L*B;
    }
    int perimeter()
    {
           return 2*(L+B);
    }
 }

 class Test
{
	
		public static void main(String arg[])
		{
			
			Rectangle r1=new Rectangle();
			r1.show();
			System.out.println("area of rectangle="+r1.area());
			System.out.println("perimeetr of rectangle="+r1.perimeter());
			Rectangle r2=new Rectangle(4,5);
			r2.show();
			System.out.println("area of rectangle="+r2.area());
			System.out.println("perimeetr of rectangle="+r2.perimeter());
		}
}








class sqaure
{
	private int s;
	sqaure()
	{

	}
	sqaure(int x)
	{
		s=x;
	}
	void show()
	{
		System.out.println("side="+s);

	}
	int area()
	{
		return s*s;
	}
	int perimeter()
	{
		 return 4*s;

	}
}
class Test
{
	public static void main (String arg[])
	{
		sqaure s1 = new sqaure();
		s1.show();
		System.out.println("area of square="+s1.area());
		System.out.println("Perimeter of square="+s1.perimeter());
		sqaure s2 = new sqaure(4);
		s2.show();
		System.out.println("area of square="+s2.area());
		System.out.println("Perimeter of square"+s2.perimeter());

	}
}