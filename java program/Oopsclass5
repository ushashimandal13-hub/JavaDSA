Here all object datas are same.
if we provide different object different values then use parameterize constructor.


parameterized constructor
________________________________

class classname
{
	classname(formal argument)
	{

	}
}

compiler does not provide default constructor.



classname objrefername=new classname();// error

classname objrefername=new classname(actual argument);

class Student
{
	String name;
	int rollno;
	double mark;
	Student(String n,int r,double m)
	{
	    name=n;
	    rollno=r; //by default this.rollno=r;
	    this.mark=m;
	}
    void disp()
    {
       System.out.println(name+" "+rollno+" "+mark);
    }

}
class Test
{
	public static void main(String arg[])
	{

	   Student s=new Student("muna",1,90.50);
	   s.disp();
	  
	}
}
o/p
muna  1 90.50


class Student
{
	String name;
	int rollno;
	double mark;
	Student(String n,int r,double m)
	{
	    name=n;
	    rollno=r; //by default this.rollno=r;
	    this.mark=m;
	}
    void disp()
    {
       System.out.println(name+" "+rollno+" "+mark);
    }

}
class Test
{
	public static void main(String arg[])
	{

	   Student s=new Student("muna",1,90.50);
	   s.disp();
	    Student s1=new Student("kuna",2,80.50);
	   s1.disp();
	  // Student s2=new Student(); error   because  no default constructor
	  // s2.disp();
	  
	}
}
