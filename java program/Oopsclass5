muna  1 90.50
kuna 2 80.50



import java.util.Scanner;
class Student
{
	String name;
	int rollno;
	double mark;
	Student(String n,int r,double m)
	{
	    name=n;
	    rollno=r; //by default this.rollno=r;
	    this.mark=m;
	}
    void disp()
    {
       System.out.println(name+" "+rollno+" "+mark);
    }

}
class Test
{
	public static void main(String arg[])
	{
       Scanner sc=new Scanner(System.in);
       System.out.println("enter name rollno and mark ");
       String nm=sc.nextLine();
        int r=sc.nextInt();
        double ma=sc.nextDouble();
	   Student s=new Student(nm,r,ma);
	   s.disp();
	  
	}
}

