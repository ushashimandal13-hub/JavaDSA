class Student
{
	String name; //instance variable
	int rollno;
	double mark;
	Student(String name,int rollno,double mark) //formal variable 
	{
	    name=name;
	    rollno=rollno; 
	    mark=mark;
	}
    void disp()
    {
       System.out.println(name+" "+rollno+" "+mark);
    }

}
class Test
{
	public static void main(String arg[])
	{

	   Student s=new Student("muna",1,90.50);
	   s.disp();
	  
	}
}
