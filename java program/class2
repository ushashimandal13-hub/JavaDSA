=  (assignment operator)

variablename=variablename/constant/expression;

a=10;
b=a;


10=a;  error     requried variable    found value
a+b=30; error  
10=20;   error

++a;
a=a+1;

++10;      error    required variable found value
10=10+1;


increment and decrement operator not applicable constant and expression



class Test
{
	public static void main(String arg[])
	{
	    int a=5;
	    System.out.println(++a);
	    //System.out.println(++10);  error 
	}
}



increment and decrement operator  not applicable boolean and string type
class Test
{
	public static void main(String arg[])
	{
	    int a=5;
	    double b=12.34;
	    char ch='x';
	    ++a;
	    ++b;
	    ++ch;
	    System.out.println(a+" "+b+" "+ch);
	  }
}

