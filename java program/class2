//swapping two no using 3rd variable
class Test
{
	public static void main(String arg[])
	{
	     int a=10,b=20,c=30,d;
	     d=c;
	     c=b;
	     b=a;
	     a=d;
	     System.out.println(a+"  "+b+" "+c);

	}
}

