int i,j;
for(i=1;i<=3;i++)
{
    for(j=1;j<=4;j++)
      {
        System.out.print(j+"\t");
      }
    System.out.println();
}


or

int i,j;
i=1;
while(i<=3)
{
    for(j=1;j<=4;j++)
      {
        System.out.print(j+"\t");
      }
    System.out.println();
    i++;
}


or
