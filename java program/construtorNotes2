class Test
{
	
	Test()
	{
	     System.out.println("zero argument constructor");
	}
	Test(int x,int y)
	{
	     System.out.println("para constructor");
	}
	public static void main(String arg[])
	{
	    Test t=new Test(10,20);
	    Test t1=new Test();
	}
}
